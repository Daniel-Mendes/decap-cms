    "react-transition-group": "catalog:"
  },
  "peerDependencies": {
    "@emotion/react": "catalog:",
    "@emotion/styled": "catalog:",
    "lodash": "catalog:",
    "prop-types": "catalog:",
    "react": "catalog:",
    "react-immutable-proptypes": "catalog:"
  }
