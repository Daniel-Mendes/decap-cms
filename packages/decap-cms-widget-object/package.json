{
  "name": "decap-cms-widget-object",
  "description": "Widget for displaying an object of fields for Decap CMS.",
  "version": "3.4.0",
  "homepage": "https://www.decapcms.org/docs/widgets/#object",
  "repository": "https://github.com/decaporg/decap-cms/tree/main/packages/decap-cms-widget-object",
  "bugs": "https://github.com/decaporg/decap-cms/issues",
  "module": "dist/esm/index.js",
  "main": "dist/decap-cms-widget-object.js",
  "license": "MIT",
  "keywords": [
    "decap-cms",
    "widget",
    "fields",
    "object",
    "nested"
  ],
  "sideEffects": false,
  "scripts": {
    "develop": "pnpm run build:esm --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "build:esm": "cross-env NODE_ENV=esm babel src --out-dir dist/esm --ignore \"**/__tests__\" --root-mode upward"
  },
  "peerDependencies": {
    "@emotion/react": "catalog:",
    "@emotion/styled": "catalog:",
    "decap-cms-lib-widgets": "workspace:*",
    "decap-cms-ui-default": "workspace:*",
    "immutable": "catalog:",
    "lodash": "catalog:",
    "prop-types": "catalog:",
    "react": "catalog:",
    "react-immutable-proptypes": "catalog:"
  }
}