{
  "name": "decap-cms-ui-auth",
  "description": "UI auth library for Decap CMS",
  "version": "3.2.3",
  "repository": "https://github.com/decaporg/decap-cms/tree/main/packages/decap-cms-ui-auth",
  "bugs": "https://github.com/decaporg/decap-cms/issues",
  "module": "dist/esm/index.js",
  "main": "dist/decap-cms-ui-auth.js",
  "license": "MIT",
  "keywords": [
    "decap-cms",
    "ui",
    "auth"
  ],
  "sideEffects": false,
  "scripts": {
    "develop": "pnpm run build:esm --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "build:esm": "cross-env NODE_ENV=esm babel src --out-dir dist/esm --ignore \"**/__tests__\" --root-mode upward --extensions \".js,.jsx,.ts,.tsx\""
  },
  "dependencies": {
    "jwt-decode": "catalog:"
  },
  "peerDependencies": {
    "@emotion/react": "catalog:",
    "@emotion/styled": "catalog:",
    "decap-cms-lib-auth": "workspace:*",
    "decap-cms-ui-default": "workspace:*",
    "lodash": "catalog:",
    "prop-types": "catalog:",
    "react": "catalog:"
  }
}